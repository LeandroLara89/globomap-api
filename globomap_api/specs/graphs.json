{

  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^([a-z_]){1,}$",
      "minimum": 1
    },
    "alias": {
      "type": "string",
      "pattern": "^([a-zA-Z_ -]){1,}$",
      "minimum": 1
    },
    "icon": {
      "type": "string",
      "pattern": "^([a-z_-]){1,}$",
      "minimum": 1
    },
    "description": {
      "type": "string",
      "pattern": "^([a-z_-]){1,}$",
      "minimum": 1
    },
    "links": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "edge": {
            "type": "string"
          },
          "from_collections": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "minItems": 1
          },
          "to_collections": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "minItems": 1
          }
        },
        "required": [
          "edge",
          "from_collections",
          "to_collections"
        ]
      }
    }
  }
}
