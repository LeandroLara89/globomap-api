{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^([a-z_]){1,}$",
      "minimum": 1
    },
    "alias": {
      "type": "string",
      "pattern": "^([a-zA-Z_ -]){1,}$",
      "minimum": 1
    },
    "icon": {
      "type": "string",
      "pattern": "^([a-z_-]){1,}$",
      "minimum": 1
    },
    "description": {
      "type": "string",
      "pattern": "^([a-z_-]){1,}$",
      "minimum": 1
    }
  },
  "required": [
    "name",
    "alias",
    "icon",
    "description"
  ]
}
