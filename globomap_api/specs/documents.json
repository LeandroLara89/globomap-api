{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "provider": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "properties": {
      "type": "array",
      "items": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "key": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "value": {
            "type": [
              "array",
              "object",
              "boolean",
              "integer",
              "string",
              "number",
              "null"
            ]
          }
        }
      }
    }
  },
  "required": [
    "id",
    "name",
    "provider",
    "timestamp"
  ]
}
