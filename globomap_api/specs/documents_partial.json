{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^([a-zA-Z0-9=._-]){1,}$",
      "minimum": 1
    },
    "name": {
      "type": "string"
    },
    "provider": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "properties": {
      "type": "object"
    },
    "properties_metadata": {
      "type": "object"
    }
  }
}
